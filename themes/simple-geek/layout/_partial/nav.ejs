<div class="hd logodiv">
    <!-- https://getavataaars.com/?accessoriesType=Prescription02&avatarStyle=Transparent&clotheColor=Gray01&clotheType=Hoodie&eyeType=Happy&eyebrowType=DefaultNatural&facialHairType=Blank&hairColor=Black&mouthType=Twinkle&skinColor=Pale&topType=ShortHairShortFlat -->
    <%- image_tag('img/wei-avatar.svg',{class: 'logopng'}) %>
    <p class="logot"><span class="logo_1">&lt;</span><span class="logo_2">/</span><span class="logo_3">#</span> <%= theme.logo_title %> <span class="logo_3">#</span><span class="logo_2">/</span><span class="logo_1">&gt;</span></p>
</div>
<div class="hd">
    <ol class="breadcrumb">
        <% for (name in theme.menu) { %>
            <% if (page.path.startsWith(theme.menu[name].slice(1)+'/')) { %>
                <li><a class="ba bold" href="<%- url_for(theme.menu[name]) %>"><%= name %></a></li>
            <% } else { %>
                <li><a class="ba" href="<%- url_for(theme.menu[name]) %>"><%= name %></a></li>
            <% } %>
        <% } %>
    </ol>
    <hr class="breadcrumbhr">
</div>